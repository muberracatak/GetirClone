			var scale_total=1;
			function scaleDown(rat){

				selectIcon();
				var my_icon=canvas.getActiveObject();
				var minu;

				if(rat=="2x"){
					my_icon.set('scaleX', scale_x_def);
					my_icon.set('scaleY', scale_y_def);
				minu=1;	
				}else{
				minu=.1	
				}
				my_icon.set('scaleX', my_icon.scaleX-minu);
				my_icon.set('scaleY', my_icon.scaleY-minu);
				scale_total=scale_total-minu;
		
				
				my_icon.setCoords();
				canvas.renderAll();
				save();	
				
			}
			
			
			function scaleUp(){
				selectIcon();
				var my_icon=canvas.getActiveObject();
				
				scale_total+=.1;
				if(scale_total>1){
					scale_total=1;
				}else{
					
				my_icon.set('scaleX', my_icon.scaleX+.1);
				my_icon.set('scaleY', my_icon.scaleY+.1);
				my_icon.setCoords();
				canvas.renderAll();
				save();	
				
				}
				
			}
			
			
