  function addCommas(nStr) {
      nStr += '';
      var x = nStr.split('.');
      var x1 = x[0];
      var x2 = x.length > 1 ? '.' + x[1] : '';
      var rgx = /(\d+)(\d{3})/;
      while (rgx.test(x1)) {
          x1 = x1.replace(rgx, '$1' + ',' + '$2');
      }
      return x1 + x2;
  }

  // only hit the api if we have something to search
  if(search_query != '' && search_query != undefined){


    // AJAX request for Videos
    $.ajax({
        type: 'GET',
        url: 'https://elements-partners-api.envato.com/api/v1/items?search_terms='+ search_query +'&item_type=graphics&categories=Icons&partner_id=3190285',
        data: {
          format: 'json'
        }
      })
      .fail(function() { console.log('API request failed') })
      .done(function(data) {
		console.log(data);
        var total_results = data['total_result_count'];



        // check we have some results
        if(total_results > 0) {

            $("#elements-results-header").append('<div class="container-fluid h-100 align-items-center"><div class="row"><div class="col-12 col-md-12 col-lg-12 text-center">'+ addCommas(total_results) +' "'+ search_query +'" icons can be found on Envato Elements  <img src="https://www.iconpacks.net/images/elements-logo2-01.svg"  alt=""/><span> Unlimited downloads of millions of creative assets</span></div></div>');

         
		 
		  var items = data['results'];
          shuffle(items);
		 
		  var item_counter = 0;
          $.each(items, function(index, item) {

            // console.log(item);
            item_counter++
            item_link = item['affiliate_link_url'];
            item_thumbnail = item['thumbnail_images']['w316'];
			

            // only show the first 10 items
            if(item_counter <= 12) {
 
				
				 owl.trigger('add.owl.carousel', [jQuery('<div class="item"><a  target="_blank" href="'+ item_link +'" rel="sponsored"><img class="img-fluid owl-image" width="316" height="211" src="' + item_thumbnail +'"/></a></div>')]);
              
            }

          });
		  
		  
		  owl.trigger('refresh.owl.carousel');
			
			//$('.owl').html(owl_content);

         // $("#elements-results-header").fadeIn('slow');

			//$("#elements-results2").fadeIn('slow');
        }
		
		
		

      });
	  
	   

  }
  

  
  
  
}); // d ready



